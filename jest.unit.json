
{
    "preset" : "ts-jest",
    "coverageDirectory" : "./coverage",
    "testMatch": [
        "**/?(*.)+(spec).ts"
    ],
    "coveragePathIgnorePatterns": [
        "./src/modules/algorithm.ts",
        "./src/routes/oauthRoutes.ts"
    ],
    "resetMocks": true,
    "clearMocks": true,
    "setupFilesAfterEnv": ["./__tests__/setup.ts"],
    "collectCoverageFrom" : ["src/**/{!(db),}/*.ts"]
}